{
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": [
      "dmg",
      "deb",
      "nsis"
    ],
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "productName": "happy",
  "version": "1.0.4",
  "identifier": "github.com.thewh1teagle.happy",
  "plugins": {},
  "app": {
    "withGlobalTauri": false,
    "windows": [
      {
        "visible": false,
        "title": "happy",
        "label": "main",
        "width": 1440,
        "height": 900
      }
    ],
    "security": {
      "csp": null
    }
  }
}